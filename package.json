{
  "name": "yyf_antd",
  "version": "0.1.1",
  "description": "react UI components",
  "author": "feidudu1",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "React",
    "Antd"
  ],
  "homepage": "https://github.com/feidudu1/yyf_antd",
  "repository": {
    "type": "git",
    "url": "https://github.com/feidudu1/yyf_antd"
  },
  "files": [
    "dist"
  ],
  "resolutions": {
    "babel-loader": "8.1.0",
    "webpack": "4.44.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "react-transition-group": "^4.4.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "clearn": "rimraf ./dist",
    "lint": "eslint --ext js,ts,tsx src",
    "build": "yarn run clearn && yarn run build-ts && yarn run build-css",
    "test": "react-scripts test",
    "test:nowatch": "cross-env CI=true react-scripts test",
    "eject": "react-scripts eject",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "node-sass ./src/styles/index.scss ./dist/index.css",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.3",
    "@storybook/addon-essentials": "^6.3.3",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.3",
    "@storybook/node-logger": "^6.3.3",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.0",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-transition-group": "^4.4.1",
    "@types/storybook__addon-info": "^5.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.3.0",
    "raf": "^3.4.1",
    "react": "^17.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "rim": "^0.0.0",
    "sass": "^1.35.2",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "typescript": "^4.2.4",
    "web-vitals": "^1.0.1"
  }
}
